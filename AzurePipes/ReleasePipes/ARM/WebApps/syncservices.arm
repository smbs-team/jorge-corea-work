      "type": "string"
    },
    "PTASMagicLinkServiceClientSecretUri": {
      "defaultValue": "https://ptas-sbox-keyvault.vault.azure.net/secrets/ptas-dev-app-sececret/87184bd8fbf84b83b361a98e0c022acd",
      "type": "string"
    },
    "PTASSautinLicenseSecretUri": {
