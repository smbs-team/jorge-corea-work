      "type": "string"
    },
    "B2CPTASRedirectAppId": {
      "defaultValue": "9ec48da8-3995-4181-8338-25da2cf1661b",
      "type": "string"
    },
    "UtilitiesApimSubscriptionKey": {
      "defaultValue": "1bcc1aed30214ca3958ec81527f3dc83",
