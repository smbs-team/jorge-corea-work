            {
              "environment": {},
              "taskId": "72a1931b-effb-4d2e-8fd8-f8472a07cb62",
              "version": "4.*",
              "name": "Role Assignment",
              "refName": "",
              "enabled": true,
              "alwaysRun": false,
              "continueOnError": false,
              "timeoutInMinutes": 0,
              "definitionType": "task",
              "overrideInputs": {},
              "condition": "succeeded()",
              "inputs": {
                "ConnectedServiceNameARM": "$(DeploymentSubscription)",
                "ScriptType": "FilePath",
                "ScriptPath": "$(System.DefaultWorkingDirectory)/_PTAS-DEV-IAC-PRBuild/NetDrop/azure/PipelineScripts/RoleAssignment.ps1",
                "Inline": "$deployEnv = 'Dev'\n$resGroupName = 'ptas-dev-resourcegroup'\n$vaultName = \"PTAS-$($deployEnv)-KeyVault\"\n\nfunction AssignRole()\n{\n\tparam( $principalName, $roleName, $resourceName, $resourceType, $resourceGroupName)\n\n\twrite-output \"-> Reading $principalName Service Principal\"\n\t\n\t$servicePrincipal = Get-AzADServicePrincipal -SearchString $principalName\n\n\twrite-output \"-> Adding $roleName role for $resourceName ...\"\n\n\t$currentRole = Get-AzRoleAssignment -ObjectId $servicePrincipal.Id -RoleDefinitionName $roleName -ResourceName $resourceName -ResourceType $resourceType -ResourceGroupName $resourceGroupName\n\n\tif ($currentRole -eq $null)\n\t{\n\t   New-AzRoleAssignment -ObjectId $servicePrincipal.Id -RoleDefinitionName $roleName -ResourceName $resourceName -ResourceType $resourceType -ResourceGroupName $resourceGroupName\n\t}\n}\n\nfunction AddVaultAccess()\n{\t\n\tparam( $principalName, $keyvaultName )\n\n\twrite-output \"-> Reading $principalName Service Principal\"\n\t\n\t$servicePrincipal = Get-AzADServicePrincipal -SearchString $principalName\n\n\twrite-output \"-> Adding KeyVault access policy for $principalName\"\n\t\n\tSet-AzKeyVaultAccessPolicy -VaultName  $keyvaultName -ObjectId $servicePrincipal.Id -PermissionsToSecrets Get\n}\n\nfunction AddSqlServerAccess()\n{\n\t#write-output \"--> Adding datareader role for SQL Server...\"\n\t#$readOnlyAdGroup = Get-AzADGroup -SearchString \"Azure-PTAS-[Env]-DBReadOnly\"\n\t#Add-AzADGroupMember -MemberObjectId  $servicePrincipal.Id -TargetGroupObjectId $readOnlyAdGroup.Id\n}\n\n# --- Map Tile Services role assignment ---\n$servicePrincipalName = \"PTAS-$($deployEnv)-MapTileServices\"\nAssignRole -principalName $servicePrincipalName -roleName \"Storage Blob Data Contributor\" -resourceName \"ptas$($deployEnv.ToLower())storagepremium\" -resourceType \"Microsoft.Storage/storageAccounts\" -resourceGroupName $resGroupName\nAddVaultAccess -principalName $servicePrincipalName -keyvaultName $vaultName\nAddSqlServerAccess\n\n# --- Data Services role assignment ---\n$servicePrincipalName = \"PTAS-$($deployEnv)-DataServices\"\nAssignRole -principalName $servicePrincipalName -roleName \"Storage Blob Data Contributor\" -resourceName \"ptas$($deployEnv.ToLower())storage\" -resourceType \"Microsoft.Storage/storageAccounts\" -resourceGroupName $resGroupName\nAddVaultAccess -principalName $servicePrincipalName -keyvaultName $vaultName\n\n# --- Document Storage Services role assignment ---\n$servicePrincipalName = \"PTAS-$($deployEnv)-DocumentStorageServices\"\nAssignRole -principalName $servicePrincipalName -roleName \"Storage Blob Data Contributor\" -resourceName \"ptas$($deployEnv.ToLower())storage\" -resourceType \"Microsoft.Storage/storageAccounts\" -resourceGroupName $resGroupName\nAssignRole -principalName $servicePrincipalName -roleName \"Cognitive Services User\" -resourceName \"PTAS-$($deployEnv)-ComputerVision\" -resourceType \"Microsoft.CognitiveServices/accounts\" -resourceGroupName $resGroupName\nAddVaultAccess -principalName $servicePrincipalName -keyvaultName $vaultName\n\n\n",
                "ScriptArguments": "-DeploymentEnv $(DeploymentEnv) -DeploymentResourceGroup $(DeploymentResourceGroup)",
                "errorActionPreference": "stop",
                "FailOnStandardError": "false",
                "TargetAzurePs": "LatestVersion",
                "CustomTargetAzurePs": "",
                "pwsh": "false",
                "workingDirectory": ""
              }
            }
