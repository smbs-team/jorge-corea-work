    <PackageReference Include="System.Drawing.Common" Version="5.0.1" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\BaseWorkerLibrary\BaseWorkerLibrary.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Script\FolderCopy.bat">
